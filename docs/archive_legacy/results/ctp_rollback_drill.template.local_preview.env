ROLLBACK_ENV_NAME=single-host-ubuntu
ROLLBACK_TRIGGER_CONDITION=order_latency_p99_ms_gt_5ms_or_reconnect_p99_s_gt_10s
NEW_CORE_ENGINE_STOP_CMD=bash -lc 'echo local-preview: skip stop new core'
NEW_STRATEGY_RUNNER_STOP_CMD=bash -lc 'echo local-preview: skip stop new runner'
RESTORE_PREVIOUS_BINARIES_CMD=bash -lc 'echo local-preview: skip restore binaries'
RESTORE_REDIS_BRIDGE_COMPAT_CMD=bash -lc 'echo local-preview: skip restore redis compat'
PREVIOUS_CORE_ENGINE_START_CMD=bash -lc 'echo local-preview: skip start previous core'
PREVIOUS_STRATEGY_RUNNER_START_CMD=bash -lc 'echo local-preview: skip start previous runner'
POST_ROLLBACK_VALIDATE_CMD=bash -lc 'echo local-preview: skip rollback validate'
MAX_ROLLBACK_SECONDS=180
ROLLBACK_EVIDENCE_OUTPUT=docs/results/ctp_rollback_result.local_preview.env
